      "/build/",
      "/tests/utils/",
      "/node_modules/"
    ],
    "rootDir": "",
    "unmockedModulePathPatterns": [],
    "roots": [
      "<rootDir>"
    ],
    "testRegex": "(/tests/unit/.*|(\\.|/)(test))\\.js?$",
    "collectCoverage": true,
    "testURL": "http://localhost/"
  }
